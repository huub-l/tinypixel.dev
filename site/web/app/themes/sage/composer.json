{
  "name": "roots/sage",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme with a modern development workflow",
  "homepage": "https://roots.io/sage/",
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:pixelcollective/acorn-github-service-provider"
    },
    {
    "type": "git",
      "url": "git@github.com:pixelcollective/acorn-markdown"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "illuminate/cache": "^5.8",
    "league/commonmark": "^1.0",
    "league/commonmark-ext-autolink": "^1.0",
    "nothingworks/blade-svg": "^0.3.1",
    "roots/acorn": "dev-master",
    "stoutlogic/acf-builder": "^1.8",
    "tiny-pixel/fields-composer": "dev-master",
    "tiny-pixel/github-service-provider": "dev-master",
    "tiny-pixel/acorn-commonmark": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs"
    ],
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}
