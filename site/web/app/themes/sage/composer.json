{
  "name": "roots/sage",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme with a modern development workflow",
  "homepage": "https://roots.io/sage/",
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:pixelcollective/acorn-github-service-provider"
    },
    {
    "type": "git",
      "url": "git@github.com:pixelcollective/acorn-markdown"
    },
    {
      "type": "git",
      "url": "git@github.com:pixelcollective/acorn-db"
    },
    {
      "type": "git",
      "url": "git@github.com:pixelcollective/acorn-media-tools"
    },
    {
      "type": "git",
      "url": "git@github.com:pixelcollective/acorn-settings-admin-menus"
    },
    {
      "type": "git",
      "url": "git@github.com:pixelcollective/acorn-settings-widgets"
    },
    {
      "type": "git",
      "url": "git@github.com:pixelcollective/acorn-settings-gutenberg"
    },
    {
      "type": "git",
      "url": "git@github.com:pixelcollective/acorn-settings-roles"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "nothingworks/blade-svg": "^0.3.1",
    "roots/acorn": "dev-view-composer-traits",
    "stoutlogic/acf-builder": "^1.8",
    "tiny-pixel/acorn-commonmark": "dev-master",
    "tiny-pixel/acorn-db": "dev-master",
    "tiny-pixel/acorn-mail": "dev-illuminate-queue-and-bus",
    "tiny-pixel/github-service-provider": "^0.0.1",
    "guzzlehttp/guzzle": "^6.3",
    "lcobucci/jwt": "^3.3",
    "php-http/guzzle6-adapter": "^1.1",
    "league/commonmark": "^1.0",
    "league/commonmark-ext-autolink": "^1.0",
    "league/commonmark-extras": "^1.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs"
    ],
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}
